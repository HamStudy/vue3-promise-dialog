{
  "name": "vue3-promise-dialog",
  "version": "0.2.1",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build-lib": "vite build --config vite-lib.config.ts && tsc lib/ts/lib.ts --declaration --emitDeclarationOnly --outDir dist",
    "preview": "vite preview"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/vue3-promise-dialog.umd.js",
  "types": "dist/lib.d.ts",
  "module": "./dist/vue3-promise-dialog.es.js",
  "exports": {
    ".": {
      "import": "./dist/vue3-promise-dialog.es.js",
      "require": "./dist/vue3-promise-dialog.umd.js"
    }
  },
  "dependencies": {
    "vue": "^3.2.26"
  },
  "devDependencies": {
    "@types/node": "^17.0.12",
    "@vitejs/plugin-vue": "^2.0.0",
    "sass": "^1.46.0",
    "typescript": "^4.5.5",
    "vite": "^2.7.13",
    "primeicons": "^5.0.0",
    "primevue": "^3.11.0",
    "tslib": "^2.3.1",
    "primeflex": "^3.1.2",
    "vue-tsc": "^0.31.1"
  },
  "keywords": [
    "vue",
    "vue3",
    "dialog",
    "modal",
    "promise"
  ]
}
